<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyFlowLayout">
        <attr name="itemSpacing" format="dimension" />
        <attr name="lineSpacing" format="dimension" />
        <attr name="expandLine" format="integer" />
    </declare-styleable>

    <declare-styleable name="OvalLabelView">
        <attr name="ovalLabelBackground" format="reference|color" />
        <attr name="labelColor" format="reference|color" />
        <attr name="label" format="reference|string" />
        <attr name="labelSize" format="reference|dimension" />
    </declare-styleable>

    <attr name="type">
        <enum name="none" value="0" />
        <enum name="circle" value="1" />
        <enum name="rounded_rect" value="2" />
    </attr>
    <attr name="rectRoundRadius" format="dimension" />
    <attr name="borderColor" format="color" />
    <attr name="borderWidth" format="dimension" />

    <declare-styleable name="CircleImageView">
        <attr name="type" />
        <attr name="borderColor" />
        <attr name="borderWidth" />
        <attr name="rectRoundRadius" />
    </declare-styleable>

</resources>